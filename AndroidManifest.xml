<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="fm.last">
<uses-permission android:name="android.permission.READ_CONTACTS"/>
<application android:icon="@drawable/icon" android:label="Last.fm" android:name="Application">
	<activity android:name="ScrobblerTestActivity"></activity>
	<service android:name="ScrobblerService"></service>
	<activity android:label="Last.fm" android:name=".radio.RadioClient" android:multiprocess="false">
	    
	<intent-filter>
<action android:name="RADIOCLIENT">
</action><category android:name="android.intent.category.DEFAULT">
</category></intent-filter></activity>
	<activity android:label="@string/app_name" android:name=".AccountSettings">
		<intent-filter>
			<action android:name="ACCOUNTSETTINGS"/>
			<category android:name="android.intent.category.DEFAULT"/>
		</intent-filter>
	</activity>

	<activity android:label="Gigs Near Here - Last.fm" android:name=".events.EventsView">
		<intent-filter>
			<action android:name="EVENTSVIEW"/>
			<category android:name="android.intent.category.DEFAULT"/>
		</intent-filter>
	</activity>
<activity android:name="MapActivity" android:label="Venue Location - Last.fm">
<intent-filter>
<action android:name="MAP_ACTION">
</action><category android:name="android.intent.category.DEFAULT">
</category></intent-filter></activity><activity android:name="ContactsView">
<intent-filter>
<action android:name="CONTACTS_ACTION">
</action><category android:name="android.intent.category.DEFAULT">
</category></intent-filter></activity><activity android:label="Friends List - Last.fm" android:name=".FriendMapperView">
<intent-filter>
<action android:name="FRIENDSVIEW">
</action><category android:name="android.intent.category.DEFAULT">
</category></intent-filter></activity><activity android:name=".tag.TagBrowser">
<intent-filter>
<action android:name="TAGBROWSER">
</action><category android:name="android.intent.category.DEFAULT">
</category></intent-filter></activity><activity android:name=".tag.TagRadio" android:label="Tag Radio">
<intent-filter>
<category android:name="android.intent.category.DEFAULT">
</category><action android:name="TAGRADIO">
</action></intent-filter></activity><activity android:name=".events.EventRadio">
<intent-filter>
<action android:name="EVENTRADIO">
</action><category android:name="android.intent.category.DEFAULT">
</category></intent-filter></activity><activity android:name=".home.Menu">
<intent-filter>
<action android:name="android.intent.action.MAIN">
</action><category android:name="android.intent.category.LAUNCHER">
</category></intent-filter></activity><activity android:name=".artist.ArtistRadio">
<intent-filter>
<action android:name="SIMILARARTIST">
</action><category android:name="android.intent.category.DEFAULT">
</category></intent-filter></activity></application>

</manifest>
